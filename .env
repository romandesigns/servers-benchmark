# Default resource allocation for benchmark servers
# Memory limits (in MB or format like 384m, 512m, 1g)
DOTNET_MEM_LIMIT=384m
BUN_ELYSIA_MEM_LIMIT=384m
BUN_HONO_MEM_LIMIT=384m
NODE_EXPRESS_MEM_LIMIT=384m
NODE_FASTIFY_MEM_LIMIT=384m

# CPU limits (decimal values like 0.5, 0.75, 1.0, 2.0)
DOTNET_CPU_LIMIT=0.75
BUN_ELYSIA_CPU_LIMIT=0.75
BUN_HONO_CPU_LIMIT=0.75
NODE_EXPRESS_CPU_LIMIT=0.75
NODE_FASTIFY_CPU_LIMIT=0.75

